    "jsonwebtoken": "^9.0.0",
    "next": "12.3.2",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "^1.8.9",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@babel/plugin-syntax-logical-assignment-operators": "^7.10.4",
    "@genql/cli": "^3.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.11.18",
    "@types/react-scroll": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
