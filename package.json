{
  "name": "oostaoo-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "jest",
    "test:cover": "jest --coverage",
    "generate:genql": "genql --schema ./graphql/schema.graphql --output ./graphql/generated/genql --watch 'graphql/schema.graphql'",
    "generate:nexus": "n --transpile-only -P nexus.tsconfig.json pages/api/graphql --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate:graphql": "npm run generate:genql && npm run generate:nexus"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@as-integrations/next": "^1.2.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@genql/runtime": "^2.10.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@prisma/client": "^4.11.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "bcrypt": "^5.1.0",
    "eslint": "8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.1.2",
    "eslint-plugin-jest-dom": "^4.0.3",
